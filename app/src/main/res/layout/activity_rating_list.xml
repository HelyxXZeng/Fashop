<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:orientation="vertical"
xmlns:app="http://schemas.android.com/apk/res-auto"
tools:context=".activity.RatingListActivity">

<LinearLayout
    android:id="@+id/relativeLayout2"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/me_background"
    android:paddingLeft="10dp"
    android:paddingRight="10dp"
    android:paddingTop="10dp"
    android:paddingBottom="5dp"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <ImageButton
            android:id="@+id/backBtn"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:background="@null"
            android:src="@drawable/back_orange" />

        <TextView
            android:layout_marginLeft="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="Ratings"
            android:textColor="@color/black"
            android:textSize="20sp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_margin="7dp"
            android:layout_weight="1"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:orientation="horizontal"
            android:padding="5dp"
            android:background="@drawable/size_bg"
            android:id="@+id/star5Btn"
            android:layout_gravity="center">

            <TextView
                android:layout_gravity="center_vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="12sp"
                android:text="5"
                android:textColor="@color/black"/>

            <ImageView
                android:layout_gravity="center_vertical"
                android:layout_width="16dp"
                android:layout_height="16dp"
                android:src="@drawable/star_yellow"/>


            <TextView
                android:layout_marginLeft="3dp"
                android:layout_gravity="center_vertical"
                android:id="@+id/tvQuantity5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="12sp"
                android:text="(0)"
                android:textColor="@color/black"/>


        </LinearLayout>

        <LinearLayout
            android:layout_margin="7dp"
            android:layout_weight="1"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:orientation="horizontal"
            android:padding="7dp"
            android:background="@drawable/size_bg"
            android:id="@+id/star4Btn">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="12sp"
                android:text="4"
                android:textColor="@color/black"/>

            <ImageView
                android:layout_width="16dp"
                android:layout_height="16dp"
                android:src="@drawable/star_yellow"
                android:layout_centerHorizontal="true"/>


            <TextView
                android:layout_marginLeft="3dp"
                android:id="@+id/tvQuantity4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="12sp"
                android:text="(0)"
                android:textColor="@color/black"/>


        </LinearLayout>

        <LinearLayout
            android:layout_margin="7dp"
            android:layout_weight="1"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:orientation="horizontal"
            android:padding="7dp"
            android:background="@drawable/size_bg"
            android:id="@+id/star3Btn">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="12sp"
                android:text="3"
                android:textColor="@color/black"/>

            <ImageView
                android:layout_width="16dp"
                android:layout_height="16dp"
                android:src="@drawable/star_yellow"
                android:layout_centerHorizontal="true"/>


            <TextView
                android:layout_marginLeft="3dp"
                android:id="@+id/tvQuantity3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="12sp"
                android:text="(0)"
                android:textColor="@color/black"/>


        </LinearLayout>

        <LinearLayout
            android:layout_margin="7dp"
            android:layout_weight="1"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:orientation="horizontal"
            android:padding="7dp"
            android:background="@drawable/size_bg"
            android:id="@+id/star2Btn">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="12sp"
                android:text="2"
                android:textColor="@color/black"/>

            <ImageView
                android:layout_width="16dp"
                android:layout_height="16dp"
                android:src="@drawable/star_yellow"
                android:layout_centerHorizontal="true"/>


            <TextView
                android:layout_marginLeft="3dp"
                android:id="@+id/tvQuantity2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="(0)"
                android:textColor="@color/black"
                android:textSize="12sp" />


        </LinearLayout>

        <LinearLayout
            android:layout_margin="7dp"
            android:layout_weight="1"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:orientation="horizontal"
            android:padding="7dp"
            android:background="@drawable/size_bg"
            android:id="@+id/star1Btn">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="12sp"
                android:text="1"
                android:textColor="@color/black"/>

            <ImageView
                android:layout_width="16dp"
                android:layout_height="16dp"
                android:src="@drawable/star_yellow"
                android:layout_centerHorizontal="true"/>


            <TextView
                android:layout_marginLeft="3dp"
                android:id="@+id/tvQuantity1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="12sp"
                android:text="(0)"
                android:textColor="@color/black"/>


        </LinearLayout>

    </LinearLayout>


</LinearLayout>

<View
    android:layout_marginTop="10dp"
    android:layout_width="match_parent"
    android:layout_height="5dp"
    android:layout_gravity="center_horizontal"
    android:background="@color/lightgray1"/>

    <FrameLayout
        android:id="@+id/frame_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="10dp"/>

</LinearLayout>

